<?xml version="1.0" encoding="UTF-8"?>
<Page>
    <Module id="module-header">
        <Class>HeaderModule</Class>
        <Renderers>
            <Renderer id="view-1" default="true">renderDefault</Renderer>
        </Renderers>
    </Module>
    <Module id="module-footer">
    	<Class>FooterModule</Class>
    	<Renderers>
    		<Renderer id="view-1" default="true">renderDefault</Renderer>
    	</Renderers>
    </Module>
